{
  "name": "Copy and Paste Bot",
  "description":
    "Copy info and paste it into a form.",
  "version": "0.1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Copy and Paste Bot",
    "default_icon": "red.png"
  },
  "externally_connectable": {
    "matches": ["*://localhost/*", "*://www.google.com.br/*"]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
